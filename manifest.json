{
  "name": "Team2 Tab Extension",
  "version": "0.3",
  "description": "Simple Tab Manager with directories and drag'n'drop functions",
  "permissions": [
    "storage",
    "history",
    "declarativeContent",
    "activeTab",
    "topSites",
    "tabs",
    "chrome://favicon/",
    "<all_urls>",
    "unlimitedStorage"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "icons": {
    "128": "icons/main.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources": ["thumb_no_scroll.css"],
  "manifest_version": 2
}
